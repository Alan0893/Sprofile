{
  "name": "sprofile",
  "version": "1.0.0",
  "description": "Spotify Profile App with Backend and Frontend",
  "scripts": {
    "start": "node server/index.js",
    "install-server": "npm install",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "deploy": "npm run install-server && npm run install-client && npm run build-client"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "querystring": "^0.2.1",
    "request": "^2.88.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
